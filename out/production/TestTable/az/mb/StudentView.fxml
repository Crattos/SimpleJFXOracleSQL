<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="505.0" prefWidth="745.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="az.mb.ViewController">
  <children>
      <TabPane prefHeight="323.0" prefWidth="745.0" rotateGraphic="true" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="UZYTKOWNICY">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                      <TableView fx:id="tUzytkownicy" onMouseClicked="#selectRow" prefHeight="286.0" prefWidth="745.0" tableMenuButtonVisible="true">
                        <columns>
                          <TableColumn fx:id="id"       prefWidth="100.0" text="ID" />
                          <TableColumn fx:id="nick"     prefWidth="100.0" text="NICK" />
                          <TableColumn fx:id="email"     prefWidth="100.0" text="EMAIL" />
                          <TableColumn fx:id="isAdmin"   prefWidth="100.0" text="ADMIN" />
                        </columns>
                      </TableView>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="DZIALY">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="tDzialy" prefHeight="286.0" prefWidth="745.0" tableMenuButtonVisible="true">
                          <columns>
                              <TableColumn  fx:id="id_Dzialu"    prefWidth="150.0" text="ID_DZIALU" />
                              <TableColumn  fx:id="idUzytkownika"  prefWidth="150.0" text="ID_UZYTKOWNIKA" />
                              <TableColumn  fx:id="nazwa" prefWidth="150.0" text="NAZWA" />
                          </columns>
                        </TableView>
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
    <HBox id="HBox" alignment="CENTER" layoutX="27.0" layoutY="428.0" spacing="5.0">
      <children>
        <Button id="insert" fx:id="btnInsert" defaultButton="true" mnemonicParsing="false" onAction="#insert" prefHeight="38.0" prefWidth="66.0" text="Add" />
        <Button id="delete" fx:id="btnDelete" mnemonicParsing="false" onAction="#delete" prefHeight="38.0" prefWidth="66.0" text="Delete" />
        <Button id="delete" fx:id="btnRefresh" mnemonicParsing="false" onAction="#refresh" prefHeight="38.0" prefWidth="66.0" text="Refresh" />
      </children>
    </HBox>
  </children>
</AnchorPane>
